{
  "name": "@redlibs/x-password-bundle",
  "version": "1.1.0",
  "description": "Password integration with Apollo Bundle using X-Framework.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/bluelibs"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "test": "jest --verbose dist/__tests__/index.js --passWithNoTests",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "gen-doc": "typedoc ./src/index.ts --exclude ./src/__tests__ --out typeDocs --tsconfig tsconfig.json"
  },
  "peerDependencies": {
    "@redlibs/core": "^1.0.0",
    "@redlibs/email-bundle": "^1.0.0",
    "@redlibs/graphql-bundle": "^1.0.0 || ^2.0.0",
    "@redlibs/mongo-bundle": "^1.0.0",
    "@redlibs/password-bundle": "^1.0.0",
    "@redlibs/security-bundle": "^1.0.0",
    "@redlibs/security-mongo-bundle": "^1.0.0",
    "@redlibs/validator-bundle": "^1.0.0",
    "@redlibs/x-bundle": "^1.0.0"
  },
  "devDependencies": {
    "@redlibs/core": "^1.3.0",
    "@redlibs/email-bundle": "^1.1.0",
    "@redlibs/graphql-bundle": "^1.0.0",
    "@redlibs/mongo-bundle": "^1.8.0",
    "@redlibs/nova": "^1.7.0",
    "@redlibs/password-bundle": "^1.2.0",
    "@redlibs/security-bundle": "^1.2.0",
    "@redlibs/security-mongo-bundle": "^1.3.0",
    "@redlibs/validator-bundle": "^1.1.0",
    "@redlibs/x-bundle": "^1.7.0",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.53",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "^29.0.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "source-map-support": "^0.5.13",
    "typescript": "^5.3.3"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
