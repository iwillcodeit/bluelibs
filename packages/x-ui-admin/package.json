    "@redlibs/ejson": "^1.0.0",
    "@redlibs/x-ui": "^1.0.0",
    "@redlibs/x-ui-i18n-bundle": "^1.2.0",
    "@redlibs/x-ui-react-router-bundle": "^1.0.4",
    "antd": "^4.12.3",
    "moment": "^2.25.3",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.1 || ^18.0.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.1",
