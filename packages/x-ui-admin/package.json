    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.17.33",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^17.0.35",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "antd": "^4.12.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-fetch": "^3.0.6",
