    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.1 || ^18.0.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.0 || ^18.0.0",
    "source-map-support": "^0.5.13",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.3"
